<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.herba.model.mapper.ContentMapper">
    <select id="GetContentByCid" resultType="Contents">
        select * from contents where cid= #{cid}
    </select>
</mapper>